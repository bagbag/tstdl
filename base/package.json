    "@types/koa__router": "12.0",
    "@types/luxon": "3.4",
    "@types/minio": "7.1",
    "@types/mjml": "4.7",
    "@types/node": "22",
    "@types/nodemailer": "6.4",
    "@types/pg": "8.11",
    "@typescript-eslint/eslint-plugin": "8.26",
    "concurrently": "9.1",
    "drizzle-kit": "0.30",
    "eslint": "9.21",
