{
  "name": "tstdl-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run build:tailwind",
    "build:watch": "ng build --watch --configuration development",
    "build:tailwind": "tailwind --config projects/tstdl/angular/tailwind.config.js --output dist/tstdl/angular/tailwind.css && cat dist/tstdl/angular/tailwind.css >> dist/tstdl/angular/styles.css && rm dist/tstdl/angular/tailwind.css",
    "pub": "npm run build && npm publish dist/tstdl/angular/",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1",
    "@angular/common": "^15.1",
    "@angular/compiler": "^15.1",
    "@angular/core": "^15.1",
    "@angular/forms": "^15.1",
    "@angular/platform-browser": "^15.1",
    "@angular/platform-browser-dynamic": "^15.1",
    "@angular/router": "^15.1",
    "@tstdl/base": "^0.82.0",
    "chroma-js": "^2.4.2",
    "luxon": "^3.2",
    "rxjs": "^7.8",
    "tslib": "^2.5",
    "zone.js": "^0.12"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1",
    "@angular-eslint/builder": "15.2",
    "@angular-eslint/eslint-plugin": "15.2",
    "@angular-eslint/eslint-plugin-template": "15.2",
    "@angular-eslint/schematics": "15.2",
    "@angular-eslint/template-parser": "15.2",
    "@angular/cli": "15.1",
    "@angular/compiler-cli": "15.1",
    "@types/chroma-js": "2.4",
    "@types/luxon": "3.2",
    "@typescript-eslint/eslint-plugin": "5.53",
    "@typescript-eslint/parser": "5.53",
    "eslint": "8.34",
    "ng-packagr": "15.1",
    "tailwindcss": "3.2",
    "typescript": "4.9"
  }
}
