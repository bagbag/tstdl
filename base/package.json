    "tsc-alias": "1.8",
    "typescript": "5.8"
  },
  "peerDependencies": {
    "@elastic/elasticsearch": "^8.17",
    "@google/generative-ai": "^0.24",
    "@tstdl/angular": "^0.92",
    "@zxcvbn-ts/core": "^3.0",
    "@zxcvbn-ts/language-common": "^3.0",
    "@zxcvbn-ts/language-de": "^3.0",
    "@zxcvbn-ts/language-en": "^3.0",
    "chroma-js": "^2.6",
    "drizzle-orm": "^0.40",
    "handlebars": "^4.7",
    "minio": "^8.0",
    "mjml": "^4.15",
    "mongodb": "^6.14",
    "nodemailer": "^6.10",
    "pg": "^8.13",
    "playwright": "^1.51",
    "preact": "^10.26",
    "preact-render-to-string": "^6.5",
    "undici": "^7.4",
    "urlpattern-polyfill": "^10.0"
  },
  "peerDependenciesMeta": {
